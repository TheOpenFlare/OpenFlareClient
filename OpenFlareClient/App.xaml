<Application x:Class="OpenFlareClient.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:OpenFlareClient"
             StartupUri="OF_MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/OF_Styles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <BitmapImage x:Key="OpenFlareIcon" CreateOptions="IgnoreImageCache" CacheOption="OnLoad" UriSource="Resources\OpenFlare.ico"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
